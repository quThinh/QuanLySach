<% include template/header  %>
		<!-- header end -->
        <div class="ish-header__logo">
            <a class="ish-header__link" style="margin-left: 577px; margin-top: 100px; font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif; font-size: 50px;">Những cuốn sách mọi người vừa xem</a>
        </div>
        <div class="ish-row ish-content ish-portfolio ish-macy" style="margin-top: 70px; margin-left: 20px;">
            <% arrayId.forEach(function(idsanpham){ %>
            <% data.forEach(function (spchitiet){ %>
            <% if(spchitiet._id.equals(idsanpham)){ %>
            <!-- lead -->
            <a class="ish-portfolio-item ish--tc13 ish-portfolio-<%= spchitiet.danhmuc %>" href="/chi-tiet/<%= spchitiet.ten %>.<%= spchitiet._id %>">
				<div class="ish-portfolio-item__image-container" style="margin-top:300px">
					<img class="ish-portfolio-item__image" src="<%= spchitiet.anhsp[0]%>" alt="business cards">
				</div>
				<div class="ish-portfolio-item__desc">
					<span class="ish-portfolio-item__title"><%= spchitiet.ten  %></span>
					<span class="ish-portfolio-item__subtitle"><%= spchitiet.danhmuc  %></span>
				</div>
			</a>    
            <% }}) %>
            <% }) %>
        </div>


      
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<% include template/footer  %>